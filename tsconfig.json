{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"module": "ES2020",
		"target": "ES2020",
		"moduleResolution": "node",
		"lib": [
			"ES2020",
			"DOM"
		],
		"types": [
			"svelte"
		],
		"allowImportingTsExtensions": true
	},
	"include": [
		"src/**/*"
	],
	"exclude": [
		"node_modules/*"
	]
}